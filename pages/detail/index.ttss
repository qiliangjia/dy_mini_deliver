.layout {
    width: 100%;
    min-height: 100vh;
    background-color: #F5F5F5;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.img {
    width: 100%;
}

.title {
    width: 100%;
    font-size: 50rpx;
    font-weight: 500;
    padding: 40rpx;
}

.content {
    width: 100%;
    color: #666666;
    font-size: 30rpx;
    padding: 0 40rpx;
    margin-bottom: 150rpx;
}